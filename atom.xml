<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>The Cellar Room</title>
 <link href="http://TheCellarRoom.github.io/atom.xml" rel="self"/>
 <link href="http://TheCellarRoom.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://TheCellarRoom.github.io/</id>
 <author>
   <name>The Cellar Room</name>
 </author>
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://TheCellarRoom.github.io/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://thecellarroom.net/blog/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}
       <p>
     <a href="http://TheCellarRoom.github.io/{{ post.id }}"><i class="icon-star"></i></a> 
     | Support TheCellarRoom by <a target="_blank" href="http://thechels.co.uk/coral"> betting with Coral</a>
     | <a target="_blank" href="http://thechels.co.uk/amazon">shopping at Amazon</a></p>
     </content>
 </entry>
 {% endfor %}
</feed>
